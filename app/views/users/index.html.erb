<p id="notice"><%= notice %></p>

<h1><%= User.model_name.human %></h1>

<table>
  <thead>
    <tr>
      <th><%= "email" %></th>
      <th><%= "zipcode" %></th>
      <th><%= "address" %></th>
      <th><%= "introduction" %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.zipcode %></td>
        <td><%= user.address %></td>
        <td><%= user.introduction %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @users %>



